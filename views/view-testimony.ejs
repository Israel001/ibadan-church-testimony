                <a href="#" class="left fs-13 f-700 transform-v-center">
                  <i class="fas fa-long-arrow-alt-left mr-20"></i>PREV</a
                >
                <% } %>

                <% if (testimonies.nextTestimony) { %>
                    <a href="/testimony/<%= testimonies.nextTestimony.uuid %>" class="right fs-13 f-700 transform-v-center">
                    NEXT<i class="fas fa-long-arrow-alt-right ml-20"></i>
                    </a>
