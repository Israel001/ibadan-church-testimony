                <% if (testimonies.previousTestimony) { %>
                    <a href="/testimony/<%= testimonies.previousTestimony.uuid %>" class="left fs-13 f-700 transform-v-center">
                    <i class="fas fa-long-arrow-alt-left mr-20"></i>PREV
                    </a>
