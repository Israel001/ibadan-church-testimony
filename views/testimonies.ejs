      <div class="content">
        <div>
          <div class="container">
            <h1 class="title">Customer Testimonies</h1>

            <form method="GET" action="/admin/testimonies" class="filter-form" id="filterForm">
              <label for="status">Filter by Status:</label>
              <select name="status" id="status" onchange="submitFilter()">
                <option value="" <%= !query?.status ? 'selected' : '' %>>All</option>
                <option value="PENDING" <%= query?.status === 'PENDING' ? 'selected' : '' %>>Pending</option>
                <option value="APPROVED" <%= query?.status === 'APPROVED' ? 'selected' : '' %>>Approved</option>
                <option value="REJECTED" <%= query?.status === 'REJECTED' ? 'selected' : '' %>>Rejected</option>
              </select>
            </form>

            <div class="testimonies-container">
              <% testimonies.forEach((testimony) => { %>
                <a href="/admin/view-testimony/<%= testimony.uuid %>" style="text-decoration: none; color:black">
                  <div class="testimony-card">
                    <img src="<%= testimony.image %>" alt="<%= testimony.firstname %> <%= testimony.lastname %>" class="testimony-image" />
                    <h2 style="color: black;"><%= testimony.firstname %> <%= testimony.lastname %></h2>
                    <p><strong>Address:</strong> <%= testimony.address %></p>
                    <p><strong>Status:</strong> <%= testimony.status %></p>
                    <p class="testimony-message" id="message-<%= testimony.uuid %>"><%= testimony.testimony.replace(/<\/?[^>]+(>|$)/g, '') %></p>
                  </div>
                </a>
              <% }); %>
            </div>

            <div class="pagination">
              <% if (pagination.currentPage > 1) { %>
                <a href="?page=<%= pagination.currentPage - 1 %>&limit=<%= query?.limit || 20 %>&status=<%= query?.status || '' %>">Previous</a>
              <% } %>
              <span>Page <%= pagination.currentPage %> of <%= pagination.totalPages %></span>
              <% if (pagination.currentPage < pagination.totalPages) { %>
                <a href="?page=<%= pagination.currentPage + 1 %>&limit=<%= query?.limit || 20 %>&status=<%= query?.status || '' %>">Next</a>
              <% } %>
            </div>

