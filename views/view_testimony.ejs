    <script>
     const updateTestimony = async (uuid, status) => {
     try {
        const category = document.getElementById('category').value;
        const message = document.getElementById('message').value;
        console.log('message', message);
        const rejectionReason = document.getElementById('rejectionReason').value;

        // const textarea = document.getElementById('message');
        // textarea.value = 'New value';
        // console.log(textarea.value);

        let updates = { testimony: message };

        if (status) {
          updates.status = status;
        }

        if (category !== "") {
          updates.categoryUuid = category;
        }

        if (status === 'REJECTED') {
          if (!rejectionReason) {
            alert('Please provide a reason for rejecting the testimony');
            return; // Stop further execution
          }
          updates.rejectionReason = rejectionReason; // Add rejection reason
        }

        const updateResponse = await fetch(
          `http://localhost:8080/admin/testimony/${uuid}`,
          {
            method: 'PUT',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify(updates),
          },
        );

        if (!updateResponse.ok) {
          throw new Error('Failed to update testimony');
        }

        const updatedTestimony = await updateResponse.json();
        console.log('Testimony updated successfully:', updatedTestimony);

        alert(`Testimony updates successfully!`);

        window.location.reload();
        
        return updatedTestimony;
      } catch (error) {
        console.error('Error updating testimony:', error);
        alert('Error updating testimony. Please try again later.');
        throw error; // Re-throw the error if needed
      }

    };


