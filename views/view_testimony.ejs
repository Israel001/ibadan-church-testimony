//     console.log('Update testimony button clicked');
//     updateTestimony('<%= testimony.uuid %>');
//   });
    </script>
    <script>
      document.addEventListener('DOMContentLoaded', () => {
        const uuid = '<%= testimony.uuid %>'; // Testimony UUID
        const commentsContainer = document.getElementById('comments-container');

        // Fetch Comments
        async function fetchComments() {
          try {
            const response = await fetch(`http://localhost:8080/admin/testimony/${uuid}/comments`);
            const comments = await response.json();

            // Clear existing comments
            commentsContainer.innerHTML = '';

            if (comments.length === 0) {
              commentsContainer.innerHTML = '<p class="text-body-tertiary">No comments available.</p>';
              return;
            }

            console.log('Fetched comments:', comments);

            // Render each comment
            comments.forEach((comment) => {
              const commentItem = document.createElement('div');
              commentItem.className = 'list-group-item d-flex justify-content-between align-items-start';
              commentItem.innerHTML = `
                <div>
                  <h5 class="mb-1 text-body-emphasis">${comment.name}</h5>
                  <p class="mb-1">${comment.comment}</p>
                  <small class="text-body-tertiary">${new Date(comment.createdAt).toDateString().toUpperCase()}</small>
                </div>
                <button class="btn btn-danger btn-sm" onclick="deleteComment('${comment.uuid}')">Delete</button>
              `;
              commentsContainer.appendChild(commentItem);
            });
          } catch (error) {
            console.error('Error fetching comments:', error);
            commentsContainer.innerHTML = '<p class="text-danger">Failed to load comments.</p>';
          }
        }

        async function fetchCategories() {
        try {
          const response = await fetch('http://localhost:8080/admin/allcategories');
          let allCategories = await response.json();
          console.log("allCategories", allCategories);
          allCategories = allCategories.categories;

          const categorySelect = document.getElementById('category');

          if (allCategories.length > 0) {
            allCategories.forEach((category) => {
              console.log("category", category);
              const option = document.createElement('option');
              option.value = category.uuid;
              option.text = category.name;
              categorySelect.appendChild(option);
            });
          } else {
            const option = document.createElement('option');
            option.value = '';
            option.text = 'No categories available';
            categorySelect.appendChild(option);
          }
        } catch (error) {
          console.error('Error fetching categories:', error);
        }
      }

        // Delete Comment
        async function deleteComment(commentId) {
          const uuid = '<%= testimony.uuid %>';
          if (!confirm('Are you sure you want to delete this comment?')) return;

          try {
            const response = await fetch(`http://localhost:8080/admin/testimony/${uuid}/comments/${commentId}`, {
              method: 'DELETE',
            });

            if (!response.ok) {
              throw new Error('Failed to delete comment');
            }

            alert('Comment deleted successfully');
            fetchComments();
          } catch (error) {
            console.error('Error deleting comment:', error);
            alert('Failed to delete the comment.');
          }
        }

        // Initial Load
        fetchComments();
        fetchCategories();

        // Expose deleteComment globally
        window.deleteComment = deleteComment;

        tinymce.init({
          selector: '#message',
          plugins:
            'advlist autolink lists link image charmap print preview hr anchor pagebreak',
          toolbar_mode: 'floating',
        });
      });

      const message = document.getElementById('message').value = '<%= testimony.testimony %>';

    </script>
